{
  "name": "egg-vue",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf prod",
    "dev": "webpack --watch --config ./build/webpack.conf.dev.js",
    "build": "npm run clean && webpack --config ./build/webpack.conf.prod.js",

    "start": "cross-env node index.js",
    "debug": "egg-bin dev $NODE_DEBUG_OPTION"
  },

  "dependencies": {
    "egg": "1.7.0",
    "egg-bin": "4.3.0",

    "webpack": "2.4.1",
    "webpack-merge": "4.1.0",

    "babel-loader": "7.0.0",
    "babel-core": "6.24.1",
    "babel-preset-env": "1.4.0",
    "regenerator-runtime": "0.10.3",

    "vue-loader": "12.0.0",
    "vue-template-compiler": "2.3.4",
    "css-loader": "0.26.1",
    "sass-loader": "6.0.3",
    "node-sass": "4.5.2",
    "autoprefixer": "6.7.7",

    "html-webpack-plugin": "2.28.0",

    "url-loader": "0.5.8",
    "file-loader": "0.11.1",

    "vue": "2.3.4"
  },
  "devDependencies": {
    "cross-env": "5.0.5",

    "eslint": "3.19.0",
    "eslint-plugin-html": "2.0.1",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3"
  }
}